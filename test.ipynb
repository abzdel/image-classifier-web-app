   "source": [
    "import base64\n",
    "\n",
    "# Read the image file in binary mode\n",
    "with open('tiger.jpg', 'rb') as f:\n",
    "    image_data = f.read()\n",
    "\n",
    "# Encode the image data as base64\n",
    "base64_data = base64.b64encode(image_data).decode('utf-8')\n",
    "\n",
    "# Print the base64 encoded string\n",
    "print(base64_data)\n"
   ]
  }
